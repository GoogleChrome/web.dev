{
  "name": "web.dev-frontend",
  "version": "2.0.0",
  "scripts": {
    "clean": "rm -rf dist",
    "lint:md": "remark -q -f .",
    "lint:scss": "sass-lint -v -q",
    "lint:js": "eslint . --fix",
    "build:gulp": "npx gulp build",
    "build:eleventy": "eleventy --quiet",
    "build:rollup": "node build.js",
    "build:sass": "node ./compile-css.js src/styles/all.scss dist/app.css",
    "watch:gulp": "npx gulp build && gulp watch",
    "watch:eleventy": "eleventy --watch --quiet",
    "watch:rollup": "chokidar \"src/lib/**/*.js\" -c \"npm run build:rollup\"",
    "watch:sass": "chokidar \"src/{lib,styles}/**/*.scss\" -c \"npm run build:sass\"",
    "debug:eleventy": "node --inspect-brk ./node_modules/.bin/eleventy",
    "karma": "karma start",
    "test": "run-s clean build:rollup karma",
    "watch:test": "TEST_MODE=dev npm-run-all clean build:rollup --parallel watch:rollup karma",
    "debug:test": "TEST_MODE=debug npm-run-all clean build:rollup --parallel watch:rollup karma",
    "start": "node ./server.js",
    "lint": "run-s lint:md lint:scss lint:js",
    "lhci": "./tools/lhci/run-lighthouse-ci.sh",
    "build": "npm-run-all --serial lint clean build:sass build:eleventy build:gulp build:rollup",
    "dev": "npm-run-all clean build:rollup build:sass --parallel --race watch:rollup watch:sass watch:eleventy watch:gulp start",
    "stage": "ELEVENTY_ENV=dev npm run build && gcloud app deploy --project web-dev-staging --quiet",
    "stage:personal": "ELEVENTY_ENV=dev npm run build && gcloud app deploy --project web-dev-staging --quiet --no-promote",
    "deploy": "ELEVENTY_ENV=prod npm run build && node index-algolia.js && gcloud app deploy --project web-dev-production-1 --quiet",
    "snapshots": "percy exec -- node tools/percy/snapshots.js",
    "test:snapshots": "ELEVENTY_ENV=test npm-run-all --serial build --parallel --race start snapshots"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.3.0",
    "algoliasearch": "^3.35.0",
    "babel-eslint": "^10.0.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "escape-string-regexp": "^2.0.0",
    "express": "^4.16.4",
    "focus-visible": "^5.0.2",
    "idb-keyval": "^3.2.0",
    "iso-639-1": "^2.1.0",
    "js-yaml": "^3.13.1",
    "lit-element": "^2.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-virtual": "^1.0.1",
    "terser": "^4.3.9",
    "unistore": "^3.4.1",
    "wicg-inert": "^3.0.1",
    "workbox-build": "^5.0.0",
    "workbox-cacheable-response": "^5.0.0",
    "workbox-expiration": "^5.0.0",
    "workbox-google-analytics": "^5.0.0",
    "workbox-precaching": "^5.0.0",
    "workbox-routing": "^5.0.0",
    "workbox-strategies": "^5.0.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@percy/script": "^1.0.3",
    "ansi-colors": "^3.2.4",
    "autoprefixer": "^9.6.1",
    "chalk": "^2.4.2",
    "chokidar-cli": "^1.2.2",
    "common-tags": "^1.8.0",
    "dotenv": "^8.1.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.11.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-rename": "^1.4.0",
    "husky": "^3.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^7.0.0",
    "intl-list-format": "^1.0.3",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "luxon": "^1.12.1",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "markdown-it-attrs": "^3.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.0",
