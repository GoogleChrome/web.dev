{
  "name": "web.dev-frontend",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm run styles",
    "clean": "rm -rf dist",
    "styles": "rm -rf src/styles && git clone https://github.com/googlechrome/web.dev-frontend src/styles && rm -rf src/styles/.git",
    "lint": "gulp lint-js && gulp lint-scss",
    "build:gulp": "npx gulp build",
    "build:eleventy": "eleventy",
    "watch:gulp": "npx gulp build && gulp watch",
    "watch:eleventy": "eleventy --watch",
    "start": "node ./server.js",
    "build": "npm run clean && npm run styles && npm run lint && npm run build:eleventy && npm run build:gulp",
    "dev": "npm run clean && npm-run-all -r --parallel watch:eleventy watch:gulp start",
    "debug": "node --inspect-brk ./node_modules/.bin/eleventy",
    "stage": "npm run build && gcloud app deploy --project web-dev-staging --quiet",
    "deploy": "ELEVENTY_ENV=prod npm run build && node lib/index.js"
  },
  "license": "Apache 2.0",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.7.1",
    "@11ty/eleventy-plugin-rss": "^1.0.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.1",
    "ansi-colors": "^3.2.4",
    "chalk": "^2.4.2",
    "common-tags": "^1.8.0",
    "eslint": "^5.10.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-prettier": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^1.3.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^7.0.0",
    "luxon": "^1.12.1",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.0.2",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.3.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "sass-lint": "^1.12.1",
    "slugify": "^1.3.4",
    "yaml-front-matter": "^4.0.0"
  }
}
