{
  "name": "web.dev",
  "version": "1.0.0",
  "description": "web.dev content and build tools",
  "repository": "https://github.com/GoogleChrome/web.dev.git",
  "author": "The Chromium Authors",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp build",
    "lint": "gulp lint",
    "format": "prettier --write --single-quote --trailing-comma all --no-bracket-spacing --arrow-parens always \"{lib,server}/{*.js,*.mjs,!(deps)/**/*.js}\"",
    "dev": "concurrently -k \"gulp watch\" \"npm:start\"",
    "deploy": "./lib/index.js --deploy",
    "start": "node --experimental-modules server/index.mjs",
    "test": "cross-env TEST=true concurrently -k \"npm:start\" \"npx mocha server/test/test.js\""
  },
  "dependencies": {
    "ansi-colors": "^3.1.0",
    "del": "^3.0.0",
    "fancy-log": "^1.3.2",
    "handlebars": "^4.0.12",
    "is-glob": "^4.0.0",
    "marked": "^0.5.2",
    "micromatch": "^3.1.10",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.2.0",
    "slug": "^0.9.2",
    "yaml": "^1.0.0-rc.8"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-prettier": "^3.0.0",
    "express": "^4.16.4",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-watch": "^5.0.1",
    "husky": "^1.2.1",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3",
    "puppeteer": "^1.11.0"
  }
}
