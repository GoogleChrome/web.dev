<script>
window.ga =
  window.ga ||
  function () {
    (ga.q = ga.q || []).push(arguments);
  };
ga.l = +new Date();
ga('create', '{{ site.analytics.ids.prod }}');
ga('set', 'transport', 'beacon');
ga('set', 'page', window.location.pathname);
ga('set', '{{ site.analytics.dimensions.TRACKING_VERSION }}', '{{ site.analytics.version}}');
ga('send', 'pageview');
</script>